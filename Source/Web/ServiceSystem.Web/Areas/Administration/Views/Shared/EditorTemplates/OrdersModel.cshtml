        @Html.LabelFor(m => m.CreatedOn, new { @class = "col-md-2" })
        @Html.EditorFor(m => m.CreatedOn, new { htmlAttributes = new { @class = "form-control col-md-9" } })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.WarrantyStatus, new { @class = "col-md-2" })
        @Html.EnumDropDownListFor(m => m.WarrantyStatus, new { @class = "form-control col-md-9" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Status, new { @class = "col-md-2" })
        @Html.EnumDropDownListFor(m => m.Status, new { @class = "form-control col-md-9" })
