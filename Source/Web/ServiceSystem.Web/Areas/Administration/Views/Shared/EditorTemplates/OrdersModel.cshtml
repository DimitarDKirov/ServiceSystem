    <div class="form-group">
        @Html.LabelFor(m => m.Solution, new { @class = "col-md-2" })
        @Html.EditorFor(m => m.Solution, new { htmlAttributes = new { @class = "form-control col-md-9" } })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.LabourPrice, new { @class = "col-md-2" })
        @Html.EditorFor(m => m.LabourPrice, new { htmlAttributes = new { @class = "form-control col-md-9" } })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.RepairStartDate, new { @class = "col-md-2" })
        @Html.EditorFor(m => m.RepairStartDate, new { htmlAttributes = new { @class = "form-control col-md-9" } })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.DeliverDate, new { @class = "col-md-2" })
        @Html.EditorFor(m => m.DeliverDate, new { htmlAttributes = new { @class = "form-control col-md-9" } })
    </div>
</div>

